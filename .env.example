# SurrealDB WebSocket URL
VITE_SURREALDB_WS_URL=wss://law.cuckoox.cn/rpc

# SurrealDB Namespace
VITE_SURREALDB_NS=ck_go

# SurrealDB Database (default database when no tenant)
VITE_SURREALDB_DB=test

# 数据库访问方式配置
# 可选值：
# - 'service-worker': 使用Service Worker方式访问数据库（默认，支持离线和后台同步）
# - 'direct': 直接连接数据库（更简单，但不支持离线功能）
VITE_DB_ACCESS_MODE=service-worker

# OIDC Configuration
VITE_OIDC_AUTHORITY=http://localhost:8082/realms/your-realm
VITE_OIDC_CLIENT_ID=your-client-id
VITE_OIDC_REDIRECT_URI=http://localhost:3000/oidc-callback
VITE_OIDC_POST_LOGOUT_REDIRECT_URI=http://localhost:3000/login
VITE_OIDC_SCOPE=openid profile email

# API URL for authentication
VITE_API_URL=http://localhost:8082